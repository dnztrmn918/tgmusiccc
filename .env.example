# Nova Music Bot Environment Variables
# Bu dosyayı .env olarak kopyalayın ve değerlerinizi girin

# Telegram API Bilgileri
API_ID=your_api_id_here
API_HASH=your_api_hash_here
BOT_TOKEN=your_bot_token_here

# Bot Sahibi
OWNER_ID=your_telegram_user_id_here
SUDO_USERS=user_id1,user_id2,user_id3

# Userbot Session (PyTgCalls için gerekli)
STRING_SESSION=your_session_string_here

# MongoDB Bağlantısı
MONGO_URI=mongodb://localhost:27017

# Spotify API (Opsiyonel)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

